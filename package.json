{
  "name": "nats-graph",
  "version": "0.1.0",
  "description": "Composable graph traversal helpers backed by NATS JetStream key-value buckets.",
  "type": "module",
  "main": "./index.js",
  "exports": {
    "./graph": "./graph/graph.js"
  },
  "engines": {
    "node": ">=20.0.0"
