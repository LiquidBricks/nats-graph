    "prepublishOnly": "node scripts/embed-build-meta.js"
  },
  "dependencies": {
    "@liquid-bricks/shared-providers": "github:LiquidBricks/shared-providers#2cb64ee848df4fe2734cb59f86d511aa0cad313b",
    "@nats-io/jetstream": "^3.0.2",
    "@nats-io/kv": "^3.0.2",
    "@nats-io/transport-node": "^3.0.2",
    "ulid": "^3.0.0"
  },
  "files": [
    "graph/",
    "config.js",
    "steps/",
    "kvProvider/",
    "build-meta.js"
  ],
  "license": "UNLICENSED",
  "keywords": [
    "nats",
    "graph",
    "jetstream",
    "gremlin"
